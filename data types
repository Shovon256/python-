name = "Panguine"
age = 8
is_student = True
weight = 32.5

print(name)
print("Data types of name", type(name))
print(age)
print("Data types of age is", type(age))
print(is_student)
print("Data types of is_student is", type(is_student))
print(weight)
print("Data types of weight is", type(weight))

age = str(age)
print("Data types of age ", type(age))
